@model WCSummerCampRegistration.Models.HomeViewModels.IndexViewModel

@{
    ViewData["Title"] = "Home Page";
}


@foreach (var category in Model.Category)
{
    <h3 class="text-success">@category.Name</h3>
    <div class="row" style="padding-bottom:50px;">
        @await Html.PartialAsync("_ThumbnailAreaPartial", Model.AvailWeek.Where(u => u.Category.Name.Equals(category.Name)))
    </div>

}

